APP_NAME=Laravel
APP_ENV=testing
APP_KEY=base64:srXuAgRYFLPpq3knZCl+UdcFYTi4Ve9binnG6c0ucAA=
APP_DEBUG=true
APP_URL=http://localhost

APP_LOCALE=en
APP_FALLBACK_LOCALE=en
APP_FAKER_LOCALE=en_US

APP_MAINTENANCE_DRIVER=file
PHP_CLI_SERVER_WORKERS=4
BCRYPT_ROUNDS=4

LOG_CHANNEL=stack
LOG_STACK=single
LOG_DEPRECATIONS_CHANNEL=null
LOG_LEVEL=debug

# Database - Using SQLite in memory for fast testing
DB_CONNECTION=sqlite
DB_DATABASE=:memory:

# Queue - Use sync for immediate processing in tests
QUEUE_CONNECTION=sync
QUEUE_FAILED_DRIVER=database-uuids

# Cache - Use array driver for testing
CACHE_DRIVER=array
CACHE_STORE=array

# Session - Use array driver for testing
SESSION_DRIVER=array
SESSION_LIFETIME=120
SESSION_ENCRYPT=false

# Broadcasting
BROADCAST_CONNECTION=log

# File Storage
FILESYSTEM_DISK=local

# Mail - Use array driver for testing
MAIL_MAILER=array
MAIL_HOST=127.0.0.1
MAIL_PORT=2525
MAIL_USERNAME=null
MAIL_PASSWORD=null
MAIL_ENCRYPTION=null
MAIL_FROM_ADDRESS=test@example.com
MAIL_FROM_NAME="Laravel Test"

# for production compatibility but not used in tests
REDIS_CLIENT=predis
REDIS_HOST=127.0.0.1
REDIS_PASSWORD=null
REDIS_PORT=6379

# Memory and execution settings for tests
MEMORY_LIMIT=512M
MAX_EXECUTION_TIME=300

# File Upload Settings for tests
UPLOAD_MAX_FILESIZE=10M
POST_MAX_SIZE=10M

# Excel export settings for tests
EXCEL_CACHE_DRIVER=memory
EXCEL_TEMPORARY_FILES_PATH=/tmp

# AWS (not needed for tests)
AWS_ACCESS_KEY_ID=
AWS_SECRET_ACCESS_KEY=
AWS_DEFAULT_REGION=us-east-1
AWS_BUCKET=
AWS_USE_PATH_STYLE_ENDPOINT=false

VITE_APP_NAME="${APP_NAME}"